<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.tzly.ctcyh.cargo">

    <application
        android:name=".CargoApplication"
        android:allowBackup="true"
        android:label="@string/cargo_app_name"
        android:supportsRtl="true"
        android:theme="@style/cargo_AppBaseTheme">

        <!--加油-->
        <activity
            android:name=".refuel_v.RefuelOilActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".refuel_v.RechargeAgreementActivity"
            android:screenOrientation="portrait"/>

    </application>

</manifest>
