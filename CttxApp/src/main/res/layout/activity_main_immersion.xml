<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.zantong.mobilecttx.home.activity.HomeMainActivity">

    <FrameLayout
        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/colorLineGray_e5"/>

    <cn.qqtheme.framework.widght.tablebottom.UiTableBottom
        android:id="@+id/custom_bottom"
        android:layout_width="match_parent"
        android:layout_height="@dimen/ds_147"
        android:background="@color/colorWhite"/>

</LinearLayout>
